package com.model;

public class Employee {
    private int empno;
    private String empname;
    private String doj;    // storing date as String in yyyy-MM-dd format for simplicity
    private String gender;
    private double bsalary;

    public Employee() {
    }

    public Employee(int empno, String empname, String doj, String gender, double bsalary) {
        this.empno = empno;
        this.empname = empname;
        this.doj = doj;
        this.gender = gender;
        this.bsalary = bsalary;
    }

    public int getEmpno() {
        return empno;
    }

    public void setEmpno(int empno) {
        this.empno = empno;
    }

    public String getEmpname() {
        return empname;
    }

    public void setEmpname(String empname) {
        this.empname = empname;
    }

    public String getDoj() {
        return doj;
    }

    public void setDoj(String doj) {
        this.doj = doj;
    }

    public String getGender() {
        return gender;
    }

    public void setGender(String gender) {
        this.gender = gender;
    }

    public double getBsalary() {
        return bsalary;
    }

    public void setBsalary(double bsalary) {
        this.bsalary = bsalary;
    }
}
